{
    "name": "alshenetsky/easyadmin-breadcrumbs",
    "description": "A bundle that allows you to add breadcrumbs to EasyAdmin",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.0",
        "easycorp/easyadmin-bundle": "^4.5",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|7.0",
        "symfony/config": "^5.4|^6.0|^7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alshenetsky\\EasyAdminBreadcrumbs\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alexey Alshenetsky",
            "email": "alshenetsky@icloud.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.10"
    }
}
